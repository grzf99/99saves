{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": 0,
    "import/no-dynamic-require": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/react-in-jsx-scope": 0,
    "react/prop-types": 0
  },
  "globals": {
    "window": true,
    "FB": true,
    "fetch": true
  }
}
